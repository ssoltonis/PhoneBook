<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="false"
      internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log" >

  <variable name="myvar" value="myvalue"/>

  <targets>
    <target xsi:type="File" name="infologger" fileName="${basedir}/logs/${shortdate}.log" layout="${longdate} | ${aspnet-session:Variable=UserId} | ${aspnet-sessionid} | [${uppercase:${level}}]: ${message}" />
  </targets>

  <rules>
    <logger name="InfoLogger" levels="Debug,Info,Error" writeTo="infologger" />
  </rules>
</nlog>